{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "rm -rf .source __registry__ .next && cd .. && pnpm build:packages && cd docs && pnpm build:registry && pnpm build",
  "installCommand": "pnpm install",
  "outputDirectory": ".next",
  "headers": [
    {
      "source": "/r/(.*).json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=604800, stale-while-revalidate=2592000, max-age=86400"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "public, max-age=604800"
        }
      ]
    },
    {
      "source": "/api/og(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=86400, stale-while-revalidate=604800, max-age=3600"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/llms-full.txt",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=604800, stale-while-revalidate=2592000, max-age=86400"
        }
      ]
    },
    {
      "source": "/(.*).xml",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=86400, stale-while-revalidate=604800, max-age=3600"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=86400, stale-while-revalidate=604800, max-age=3600"
        }
      ]
    }
  ]
}
