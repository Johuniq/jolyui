  "files": [
    {
      "path": "examples/sticky-navbar-icons-demo.tsx",
      "content": "import { Button } from \"@/components/ui/button\"\nimport {\n    MobileNav,\n    MobileNavHeader,\n    MobileNavMenu,\n    MobileNavToggle,\n    NavActions,\n    Navbar,\n    NavBody,\n    NavItems,\n    NavLogo,\n} from \"@/registry/default/ui/sticky-navbar\"\nimport {\n    BookOpen,\n    Code,\n    Home,\n    Package,\n    Sparkles,\n    Users,\n} from \"lucide-react\"\nimport { useState } from \"react\"\n\nexport default function StickyNavbarIconsDemo() {\n  const [mobileMenuOpen, setMobileMenuOpen] = useState(false)\n\n  const navItems = [\n    { name: \"Home\", link: \"#home\", icon: <Home className=\"h-4 w-4\" /> },\n    {\n      name: \"Features\",\n      link: \"#features\",\n      icon: <Sparkles className=\"h-4 w-4\" />,\n    },\n    {\n      name: \"Docs\",\n      link: \"#docs\",\n      icon: <BookOpen className=\"h-4 w-4\" />,\n    },\n    {\n      name: \"Components\",\n      link: \"#components\",\n      icon: <Package className=\"h-4 w-4\" />,\n    },\n  ]\n\n  return (\n    <div className=\"relative h-[600px] w-full overflow-auto\">\n      <Navbar>\n        {/* Desktop Nav */}\n        <NavBody>\n          <NavLogo href=\"#\">\n            <div className=\"flex h-8 w-8 items-center justify-center rounded-lg bg-blue-600\">\n              <Code className=\"h-4 w-4 text-white\" />\n            </div>\n            <span className=\"hidden font-semibold sm:block\">DevKit</span>\n          </NavLogo>\n\n          <NavItems items={navItems} />\n\n          <NavActions>\n            <Button variant=\"ghost\" size=\"sm\">\n              <Users className=\"mr-2 h-4 w-4\" />\n              Community\n            </Button>\n            <Button size=\"sm\" className=\"rounded-full\">\n              <Code className=\"mr-2 h-4 w-4\" />\n              Start Building\n            </Button>\n          </NavActions>\n        </NavBody>\n\n        {/* Mobile Nav */}\n        <MobileNav>\n          <MobileNavHeader>\n            <NavLogo href=\"#\">\n              <div className=\"flex h-8 w-8 items-center justify-center rounded-lg bg-blue-600\">\n                <Code className=\"h-4 w-4 text-white\" />\n              </div>\n              <span className=\"font-semibold\">DevKit</span>\n            </NavLogo>\n            <MobileNavToggle\n              isOpen={mobileMenuOpen}\n              onClick={() => setMobileMenuOpen(!mobileMenuOpen)}\n            />\n          </MobileNavHeader>\n\n          <MobileNavMenu\n            isOpen={mobileMenuOpen}\n            onClose={() => setMobileMenuOpen(false)}\n          >\n            {navItems.map((item) => (\n              <a\n                key={item.name}\n                href={item.link}\n                className=\"flex items-center gap-3 rounded-lg px-4 py-3 text-sm transition-colors hover:bg-zinc-100 dark:hover:bg-zinc-800\"\n                onClick={() => setMobileMenuOpen(false)}\n              >\n                {item.icon}\n                {item.name}\n              </a>\n            ))}\n            <hr className=\"my-2 border-zinc-200 dark:border-zinc-800\" />\n            <Button\n              variant=\"ghost\"\n              className=\"w-full justify-start\"\n              onClick={() => setMobileMenuOpen(false)}\n            >\n              <Users className=\"mr-2 h-4 w-4\" />\n              Community\n            </Button>\n            <Button\n              className=\"w-full rounded-full\"\n              onClick={() => setMobileMenuOpen(false)}\n            >\n              <Code className=\"mr-2 h-4 w-4\" />\n              Start Building\n            </Button>\n          </MobileNavMenu>\n        </MobileNav>\n      </Navbar>\n\n      {/* Demo Content */}\n      <div className=\"space-y-4 px-4 pt-32\">\n        <h1 className=\"text-4xl font-bold\">Navbar with Icons</h1>\n        {Array.from({ length: 20 }).map((_, i) => (\n          <p key={i} className=\"text-muted-foreground\">\n            Navigation items can include icons alongside text for better visual\n            hierarchy. Scroll to see the sticky effect.\n          </p>\n        ))}\n      </div>\n    </div>\n  )\n}\n",
      "type": "registry:example",
      "target": ""
    }
  ]
}