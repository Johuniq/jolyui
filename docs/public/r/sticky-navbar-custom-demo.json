  "files": [
    {
      "path": "examples/sticky-navbar-custom-demo.tsx",
      "content": "import { Button } from \"@/components/ui/button\"\nimport {\n    MobileNav,\n    MobileNavHeader,\n    MobileNavMenu,\n    MobileNavToggle,\n    NavActions,\n    Navbar,\n    NavBody,\n    NavItems,\n    NavLogo,\n} from \"@/registry/default/ui/sticky-navbar\"\nimport { Bell, Search } from \"lucide-react\"\nimport { useState } from \"react\"\n\nexport default function StickyNavbarCustomDemo() {\n  const [mobileMenuOpen, setMobileMenuOpen] = useState(false)\n\n  const navItems = [\n    { name: \"Products\", link: \"#products\" },\n    { name: \"Solutions\", link: \"#solutions\" },\n    { name: \"Resources\", link: \"#resources\" },\n    { name: \"Company\", link: \"#company\" },\n  ]\n\n  return (\n    <div className=\"relative h-[600px] w-full overflow-auto\">\n      <Navbar position=\"fixed\">\n        {/* Desktop Nav */}\n        <NavBody className=\"bg-gradient-to-r from-purple-500/10 via-pink-500/10 to-red-500/10 backdrop-blur-xl\">\n          <NavLogo href=\"#\">\n            <div className=\"flex h-10 w-10 items-center justify-center rounded-xl bg-gradient-to-br from-purple-500 to-pink-500\">\n              <span className=\"text-sm font-bold text-white\">✦</span>\n            </div>\n            <span className=\"hidden bg-gradient-to-r from-purple-600 to-pink-600 bg-clip-text font-bold text-transparent sm:block\">\n              Starlight\n            </span>\n          </NavLogo>\n\n          <NavItems items={navItems} />\n\n          <NavActions>\n            <Button variant=\"ghost\" size=\"icon\" className=\"rounded-full\">\n              <Search className=\"h-4 w-4\" />\n            </Button>\n            <Button variant=\"ghost\" size=\"icon\" className=\"rounded-full\">\n              <Bell className=\"h-4 w-4\" />\n            </Button>\n            <Button\n              size=\"sm\"\n              className=\"rounded-full bg-gradient-to-r from-purple-500 to-pink-500 text-white hover:from-purple-600 hover:to-pink-600\"\n            >\n              Launch App\n            </Button>\n          </NavActions>\n        </NavBody>\n\n        {/* Mobile Nav */}\n        <MobileNav className=\"bg-gradient-to-r from-purple-500/10 via-pink-500/10 to-red-500/10 backdrop-blur-xl\">\n          <MobileNavHeader>\n            <NavLogo href=\"#\">\n              <div className=\"flex h-10 w-10 items-center justify-center rounded-xl bg-gradient-to-br from-purple-500 to-pink-500\">\n                <span className=\"text-sm font-bold text-white\">✦</span>\n              </div>\n              <span className=\"bg-gradient-to-r from-purple-600 to-pink-600 bg-clip-text font-bold text-transparent\">\n                Starlight\n              </span>\n            </NavLogo>\n            <div className=\"flex items-center gap-2\">\n              <Button variant=\"ghost\" size=\"icon\" className=\"h-9 w-9\">\n                <Search className=\"h-4 w-4\" />\n              </Button>\n              <MobileNavToggle\n                isOpen={mobileMenuOpen}\n                onClick={() => setMobileMenuOpen(!mobileMenuOpen)}\n              />\n            </div>\n          </MobileNavHeader>\n\n          <MobileNavMenu\n            isOpen={mobileMenuOpen}\n            onClose={() => setMobileMenuOpen(false)}\n            className=\"bg-gradient-to-r from-purple-500/5 via-pink-500/5 to-red-500/5 backdrop-blur-xl\"\n          >\n            {navItems.map((item) => (\n              <a\n                key={item.name}\n                href={item.link}\n                className=\"rounded-xl px-4 py-3 text-sm font-medium transition-colors hover:bg-purple-500/10\"\n                onClick={() => setMobileMenuOpen(false)}\n              >\n                {item.name}\n              </a>\n            ))}\n            <hr className=\"my-2 border-purple-500/20\" />\n            <Button\n              className=\"w-full rounded-full bg-gradient-to-r from-purple-500 to-pink-500 text-white hover:from-purple-600 hover:to-pink-600\"\n              onClick={() => setMobileMenuOpen(false)}\n            >\n              Launch App\n            </Button>\n          </MobileNavMenu>\n        </MobileNav>\n      </Navbar>\n\n      {/* Demo Content */}\n      <div className=\"space-y-4 px-4 pt-32\">\n        <h1 className=\"text-4xl font-bold\">Custom Styled Navbar</h1>\n        {Array.from({ length: 20 }).map((_, i) => (\n          <p key={i} className=\"text-muted-foreground\">\n            This navbar uses custom styling with gradient backgrounds and custom\n            colors. Scroll to see the effect.\n          </p>\n        ))}\n      </div>\n    </div>\n  )\n}\n",
      "type": "registry:example",
      "target": ""
    }
  ]
}